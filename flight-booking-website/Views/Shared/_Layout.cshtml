<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div id="container">
        <div id="header">
        <img src="data:image/png;base64," height="75" width="900" alt="Fly with Us logo">
        </div>
        <div class="HorizLinks">
            <ul>
                <li>@Html.ActionLink("Home","Index","Home")</li>
                <li>@Html.ActionLink("View All Flights", "Flights", "Home")</li>
                <li>@Html.ActionLink("View All Bookings", "Bookings", "Home")</li>
            </ul>
        </div>
        <div id="body">
            @RenderBody()
            @Scripts.Render("~/bundles/jquery")
            @RenderSection("scripts", required: false)</div>
        <div id="footer">
        @if (Request.IsAuthenticated)
        {
            <strong>@Html.Encode(User.Identity.Name)</strong>
            @Html.ActionLink("Log Out", "LogOut", "User")
        }
        else
        {
            @Html.ActionLink("Register", "Registration", "User")
            <span>|</span>
            @Html.ActionLink("Log In", "LogIn", "User")
        }</div>
    </div>   
</body>
</html>
